@model IEnumerable<VirtualStore.Models.Employee>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Имя</th>
                <th>Фамилия</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <th>@employee.Id</th>
                    <th>@employee.Name</th>
                    <th>@employee.SecondName</th>
                    <th>@employee.Age</th>
                </tr>                
            }
        </tbody>
    </table>
</body>
</html>
