@model IEnumerable<VirtualStore.Models.Employee>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>

    <h3>Семья</h3>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Имя</th>
                <th>Фамилия</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.Name</td>
                    <td>@employee.SecondName</td>
                    <td>@employee.Age</td>
                    <td>
                        <a href="~/">Инфо</a>
                    </td>
                </tr>                
            }
        </tbody>
    </table>

    <hr/>

    <a href="~/">На главную</a>

</body>
</html>
